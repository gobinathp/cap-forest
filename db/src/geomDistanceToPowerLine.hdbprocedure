PROCEDURE "geomDistanceToPowerLine" ( IN POLYGON_AS_WKT NVARCHAR(16000), OUT MIN_DISTANCE_METER DOUBLE )
LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   READS SQL DATA AS
BEGIN 

  select top 1 geom.ST_SRID(1000004326).ST_Distance(ST_GeomFromWKT(:POLYGON_AS_WKT).ST_SRID(1000004326), 'meter') into MIN_DISTANCE_METER
  from com_test_forest_POWERLINE
  order by geom.ST_Distance(ST_GeomFromWKT(:POLYGON_AS_WKT)); 

END